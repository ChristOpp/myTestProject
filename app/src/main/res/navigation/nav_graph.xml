<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/myFragment">
    <fragment
        android:id="@+id/myFragment"
        android:name="com.example.mytestproject.FirstFragment"
        android:label="MyFragment">
        <action
            android:id="@+id/action_myFragment_to_mySecondFragment"
            app:destination="@id/mySecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/mySecondFragment"
        android:name="com.example.mytestproject.SecondFragment"
        android:label="MySecondFragment" />
</navigation>
